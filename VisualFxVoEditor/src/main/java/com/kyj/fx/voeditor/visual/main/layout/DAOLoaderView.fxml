<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.cell.MapValueFactory?>


<TitledPane maxHeight="1.7976931348623157E308" text="DAO" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kyj.fx.voeditor.visual.main.layout.DAOLoaderController">
   <content>
      <BorderPane maxHeight="1.7976931348623157E308">
      	<top>
      		<VBox>
      			<children>
						<Label text="Class Name" />
						<TextField fx:id="txtSrchTable" onKeyReleased="#txtSrchTableOnKeyReleased" />
      			</children>
      			<padding>
      				<Insets bottom="3.0" left="3.0" right="3.0" />
      			</padding>
      		</VBox>
      	</top>
      	<center>
            		<TableView fx:id="tbSrchDao" cache="false" onMouseClicked="#tbSrchDaoOnMouseClick" BorderPane.alignment="CENTER">
            			<columns>
            				<TableColumn fx:id="colSrchClassName" minWidth="100.0" prefWidth="133.0" text="ClassName">
				</TableColumn>
            				<TableColumn fx:id="colSrchPackageName" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="150.0" text="PackageName" />
            			</columns>
            			<padding>
            				<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            			</padding>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            		</TableView>
      	</center>
      </BorderPane>
   </content>
</TitledPane>
