						<Label text="기타" GridPane.rowIndex="7" />
						<TextArea fx:id="txtMessage" onKeyPressed="#txtOnKeyEvent" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
						<TextArea fx:id="txtCodeMasterTable" onKeyPressed="#txtOnKeyEvent" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
						<TextArea fx:id="txtAuthChk" onKeyPressed="#txtOnKeyEvent" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
