<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<fx:root prefHeight="785.0" prefWidth="1227.0" type="BorderPane"
	xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<SplitPane fx:id="splitMainContent" dividerPositions="0.5"
			orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0"
			BorderPane.alignment="CENTER">
			<items>
				<TabPane fx:id="tabInformations" prefHeight="200.0"
					prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
				</TabPane>
				<BorderPane fx:id="borTableColumnInformation" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" />
			</items>
		</SplitPane>
	</center>
	<bottom>
		<HBox spacing="5.0" BorderPane.alignment="CENTER">
			<children>
				<Button defaultButton="true" mnemonicParsing="false"
					prefWidth="90.0" text="도움말" disable="true" />
				<Button fx:id="btnReload" defaultButton="true"
					mnemonicParsing="false" onMouseClicked="#btnReloadOnMouseClick"
					prefWidth="90.0" text="다시불러오기" />
				<Button defaultButton="true" mnemonicParsing="false"
					prefWidth="90.0" text="저장" disable="true" />
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" top="5.0" />
			</padding>
		</HBox>
	</bottom>
</fx:root>
