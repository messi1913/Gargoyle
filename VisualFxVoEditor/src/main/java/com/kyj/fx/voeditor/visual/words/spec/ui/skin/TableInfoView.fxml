<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="803.0" prefWidth="1180.0"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.kyj.fx.voeditor.visual.words.spec.ui.skin.TableInfoController">
	<center>
		<SplitPane dividerPositions="0.6129837702871411"
			orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0"
			BorderPane.alignment="CENTER">
			<items>
				<BorderPane fx:id="borTableDefine" prefHeight="200.0"
					prefWidth="200.0">
					<bottom>
						<HBox alignment="CENTER" maxHeight="40.0" spacing="25.0"
							BorderPane.alignment="CENTER">
							<children>
								<Button fx:id="btnUp" minWidth="80.0" mnemonicParsing="false"
									onAction="#btnUpOnAction" text="Up" HBox.hgrow="NEVER" />
								<Button fx:id="btnDown" minWidth="80.0" mnemonicParsing="false"
									onAction="#btnDownOnAction" text="Down" HBox.hgrow="NEVER" />
							</children>
							<padding>
								<Insets bottom="5.0" top="5.0" />
							</padding>
						</HBox>
					</bottom>
				</BorderPane>
				<BorderPane fx:id="borTableCollect" minWidth="300.0">

				</BorderPane>
			</items>
		</SplitPane>
	</center>
</BorderPane>
