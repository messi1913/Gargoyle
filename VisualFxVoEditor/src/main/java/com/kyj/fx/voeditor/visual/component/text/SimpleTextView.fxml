		<HBox fx:id="hboxButtons" alignment="CENTER" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="btnClose" mnemonicParsing="false" prefHeight="0.0" prefWidth="123.0" text="Close" />
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</HBox>
	</bottom>
   <center>
<!--       <TextArea fx:id="txtVoEditor" prefHeight="600.0" prefWidth="800.0" BorderPane.alignment="CENTER" /> -->
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File" />
          <Menu mnemonicParsing="false" text="Edit" />
          <Menu mnemonicParsing="false" text="View">
               <items>
                  <Menu mnemonicParsing="false" text="Open Width">
                    <items>
                      <MenuItem fx:id="miOpenMsWord" mnemonicParsing="false" onAction="#miOpenMsWordOnAction" text="MsWord" />
                    </items>
                  </Menu>
               </items>
          </Menu>
        </menus>
      </MenuBar>
