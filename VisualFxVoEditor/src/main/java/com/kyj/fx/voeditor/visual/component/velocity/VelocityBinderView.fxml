<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>
<?import org.fxmisc.richtext.CodeArea?>

<fx:root prefHeight="530.0" prefWidth="501.0" styleClass="borderpane-root"
	stylesheets="@VelocityBinderView.css" type="BorderPane" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/8.0.141">
	<center>
		<SplitPane dividerPositions="0.5" orientation="VERTICAL">
			<items>
				<CodeArea fx:id="txtContext">
				</CodeArea>
				<TabPane fx:id="tabpane" side="BOTTOM">

				</TabPane>
			</items>
		</SplitPane>
	</center>
	<bottom>
		<HBox styleClass="hobx-button" BorderPane.alignment="CENTER">
			<children>
				<Button mnemonicParsing="false" onAction="#btnExecuteOnAction"
					text="Execute" />
			</children>
		</HBox>
	</bottom>
</fx:root>
