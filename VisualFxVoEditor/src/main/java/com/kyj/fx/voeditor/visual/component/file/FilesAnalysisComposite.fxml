<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<fx:root prefHeight="670.0" prefWidth="906.0" styleClass="borderpane-root"
	stylesheets="@FilesAnalysisComposite.css" type="BorderPane"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141">
	<top>
		<HBox styleClass="hbox-item-container" BorderPane.alignment="CENTER">
			<children>
				<TextField id="txtFileLocation" fx:id="txtFileLocation"
					prefHeight="23.0" prefWidth="472.0" HBox.hgrow="ALWAYS" />
				<Button id="button" mnemonicParsing="false" text="Select" />
			</children>
		</HBox>
	</top>
	<center>
		<SplitPane dividerPositions="0.5" BorderPane.alignment="CENTER">
			<items>
				<VBox styleClass="vbox-item-container">
					<children>
						<Label text="File Tree" />
						<TreeView fx:id="tvFiles" prefHeight="200.0" prefWidth="200.0"
	VBox.vgrow="ALWAYS" />
					</children>
				</VBox>
				<VBox styleClass="vbox-item-container">
					<children>
						<Label text="File Attribute" />
						<TableView prefHeight="200.0" prefWidth="200.0"
							VBox.vgrow="ALWAYS">
							<columns>
								<TableColumn prefWidth="75.0" text="C1" />
								<TableColumn prefWidth="75.0" text="C2" />
							</columns>
						</TableView>
					</children>
				</VBox>
			</items>
		</SplitPane>
	</center>
</fx:root>
