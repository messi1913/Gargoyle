<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.kyj.fx.voeditor.visual.component.scm.AddNewSVNRepositoryController">
	<center>
		<StackPane maxHeight="-Infinity" maxWidth="-Infinity"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="151.0"
			prefWidth="663.0" style="-fx-background-color: white;"
			BorderPane.alignment="CENTER">
			<children>
				<StackPane prefHeight="150.0" prefWidth="200.0">
					<children>
						<AnchorPane prefHeight="200.0" prefWidth="200.0"
							style="-fx-background-color: white; -fx-border-color: black;">
							<StackPane.margin>
								<Insets bottom="5.0" left="5.0" right="5.0" top="15.0" />
							</StackPane.margin>
							<children>
								<Label layoutX="14.0" layoutY="38.0" text="URL : " />
								<TextField fx:id="lblUrl" layoutX="87.0" layoutY="37.0"
									prefHeight="25.0" prefWidth="545.0" AnchorPane.bottomAnchor="85.0"
									AnchorPane.leftAnchor="86.0" AnchorPane.rightAnchor="22.0"
									AnchorPane.topAnchor="36.0" />
								<Label layoutX="14.0" layoutY="76.0" text="User ID:" />
								<TextField fx:id="txtUserId" layoutX="89.0" layoutY="73.0"
									prefHeight="25.0" prefWidth="543.0" AnchorPane.bottomAnchor="51.0"
									AnchorPane.leftAnchor="88.0" AnchorPane.rightAnchor="22.0"
									AnchorPane.topAnchor="72.0" />
								<Label layoutX="14.0" layoutY="112.0" text="User Pass :" />
								<PasswordField fx:id="lblUserPass" layoutX="89.0"
									layoutY="108.0" prefHeight="23.0" prefWidth="541.0" />
							</children>
						</AnchorPane>
					</children>
				</StackPane>
				<Label alignment="CENTER" style="-fx-background-color: white;"
					text="Location" StackPane.alignment="TOP_LEFT">
					<StackPane.margin>
						<Insets left="7.0" top="7.0" />
					</StackPane.margin>
					<padding>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</padding>
				</Label>
			</children>
			<BorderPane.margin>
				<Insets bottom="5.0" />
			</BorderPane.margin>
		</StackPane>
	</center>
	<bottom>
		<HBox alignment="CENTER_RIGHT" prefHeight="33.0" prefWidth="663.0"
			spacing="5.0" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="btnFinish" disable="true" mnemonicParsing="false"
					onAction="#btnFinishOnAction" prefWidth="80.0" text="Finish" />
				<Button fx:id="btnCancel" mnemonicParsing="false"
					onAction="#btnCancelOnAction" prefWidth="80.0" text="Cancel" />
			</children>
			<padding>
				<Insets right="5.0" />
			</padding>
		</HBox>
	</bottom>
</BorderPane>
