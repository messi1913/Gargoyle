<BorderPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kyj.fx.voeditor.visual.main.layout.VoEditorController">
	<top>
		<GridPane BorderPane.alignment="CENTER">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label fx:id="lblClassName" text="Class Name" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS" />
				<TextField fx:id="txtClassName" prefHeight="23.0" prefWidth="583.0" GridPane.columnIndex="1" GridPane.columnSpan="6" GridPane.rowIndex="1" />
				<Label text="Description" GridPane.rowIndex="4" />
				<TextArea fx:id="txtAreaDesc" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="6" GridPane.rowIndex="4" GridPane.rowSpan="2" />
				<Label text="Location" GridPane.rowIndex="3" />
				<TextField fx:id="txtLocation" prefHeight="23.0" prefWidth="164.0" GridPane.columnIndex="1" GridPane.columnSpan="6" GridPane.rowIndex="3" />
            <HBox GridPane.columnIndex="7" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </GridPane.margin>
               <children>
      				<Button fx:id="btnSelect" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Select" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
				<Label fx:id="lblClassName1" text="Package Name" />
				<TextField fx:id="txtPackageName" GridPane.columnIndex="1" GridPane.columnSpan="6" />
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="833.0" spacing="5.0" style="-fx-background-color: inherite;" styleClass="hbox-buttons" GridPane.columnIndex="1" GridPane.columnSpan="9" GridPane.rowIndex="6">
               <children>
      				<Button fx:id="btnGenerate" mnemonicParsing="false" prefWidth="100.0" text="Generate" />
      				<Button fx:id="btnDatabase" mnemonicParsing="false" prefWidth="100.0" text="DataBase" />
      				<Button mnemonicParsing="false" onMouseClicked="#btnExcelImporOnMouseClick" prefWidth="100.0" text="Import Excel" />
      				<Button mnemonicParsing="false" onMouseClicked="#btnExcelExportOnMouseClick" prefWidth="100.0" text="Export Excel" />
                  <Button fx:id="btnUp" mnemonicParsing="false" onMouseClicked="#btnUpOnMouseClick" prefWidth="100.0" text="Up" />
                  <Button fx:id="btnDown" mnemonicParsing="false" onMouseClicked="#btnDownOnMouseClick" prefWidth="100.0" text="Down" />
                  <CheckBox fx:id="chkWriteThenOpen" mnemonicParsing="false" prefHeight="16.0" prefWidth="154.0" selected="true" text="Export후 열기" />
               </children>
            </HBox>
            <Label text="Parent Class" GridPane.rowIndex="2" />
            <TextField fx:id="txtParentClassName" prefHeight="23.0" prefWidth="261.0" GridPane.columnIndex="1" GridPane.columnSpan="6" GridPane.rowIndex="2" />
            <HBox GridPane.columnIndex="7" GridPane.rowIndex="2">
               <children>
                  <Button fx:id="btnSelectClass" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnSelectClassOnAction" text="Select" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
			</children>
         <BorderPane.margin>
            <Insets bottom="3.0" />
         </BorderPane.margin>
		</GridPane>
	</top>
	<center>
		<TableView fx:id="tbVoEditor" editable="true" BorderPane.alignment="CENTER">
			<columns>
				<TableColumn fx:id="colName" editable="true" prefWidth="205.0" text="Name">
					<cellValueFactory>
						<PropertyValueFactory property="name" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="colType" editable="true" prefWidth="114.0" text="Type">
					<cellValueFactory>
						<PropertyValueFactory property="type" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="colSize" editable="true" prefWidth="154.0" text="Size">
					<cellValueFactory>
						<PropertyValueFactory property="size" />
					</cellValueFactory>

				</TableColumn>
				<TableColumn fx:id="colDesc" editable="true" prefWidth="606.0" text="Desc..">
					<cellValueFactory>
						<PropertyValueFactory property="desc" />
					</cellValueFactory>

				</TableColumn>
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
	</center>
	<padding>
		<Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
	</padding>
</BorderPane>
